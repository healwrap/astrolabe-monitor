{
  "name": "@healwrap/monitor-sdk-react",
  "version": "1.0.0",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./build/types/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": {
        "types": "./build/types/index.d.ts",
        "default": "./build/cjs/index.js"
      }
    }
  },
  "scripts": {
    "dev": "npm run build:watch:transpile & npm run build:watch:types",
    "build:watch:transpile": "tsup --watch",
    "build:watch:types": "tsc -p tsconfig.types.json --watch",
    "build": "npm run build:transpile && npm run build:types",
    "build:transpile": "tsup",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@healwrap/monitor-sdk-browser": "workspace:*",
    "@healwrap/monitor-sdk-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0"
  },
  "packageManager": "pnpm@10.13.1"
}
